{
  "name": "rntemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "yarn clean:ios && cd android && ./gradlew clean",
    "clean:android": "yarn clean:js && cd android && ./gradlew clean",
    "clean:ios": "yarn clean:js && cd ios && rm -rf Pods && rm -rf build && pod install",
    "clean:js": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install",
    "postinstall": "patch-package",
    "ios": "react-native run-ios",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "package:sort": "npx sort-package-json",
    "prettier": "npx prettier --config ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "start": "react-native start",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/react-native": "^5.10.0",
    "axios": "^1.5.1",
    "patch-package": "^8.0.0",
    "react-native-config": "^1.4.11",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.1.5",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-screens": "^3.25.0",
    "formik": "^2.4.5",
    "react-native-splash-screen": "^3.3.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-animatable": "1.3.3",
    "react-native-modal": "13.0.1",
    "@gorhom/bottom-sheet": "^4.5.1"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@types/styled-components-react-native": "^5.2.3",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
